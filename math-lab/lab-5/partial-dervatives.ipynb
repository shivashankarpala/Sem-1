   "source": [
    "from sympy import *\n",
    "x,y =symbols('x y')\n",
    "\n",
    "u=exp(x)*(x*cos(y)-y*sin(y))\n",
    "dux=diff(u,x)\n",
    "duy=diff(u,y)\n",
    "duxy=diff(dux,y)\n",
    "duyx=diff(duy,x)\n",
    "if duxy==duyx:\n",
    "    print('Mixed partial derivatives are equal')\n",
    "else:\n",
    "    print('Mixed partial derivatives are not equal')"
   ]
  }
 ],
