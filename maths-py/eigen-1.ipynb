    "import numpy as np\n",
    "\n",
    "I = np.array([[4,3,2],[1,4,1],[3,10,4]])\n",
    "print(\"\\nGiven Matrix: \\n\", I)\n",
    "\n",
    "w,v = np.linalg.eig(I)\n",
    "print(\"\\nEigen Values: \\n\",w)\n",
    "print(\"\\nEigen Vectors: \\n\",v)\n",
    "\n",
    "print(\"\\nEigen Value: \\n\", w[0])\n",
    "print(\"\\nCorresponding Eigen Vector: \\n\", v[:,0])"
